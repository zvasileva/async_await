{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
	"no-unused-vars": 0,
	"no-plusplus": 0,
    "import/no-unresolved": [
      2, 
      { "caseSensitive": false }
   ],
   "consistent-return": 0,
   "import/no-unresolved": 0,
   "import/extensions": 0
  }
}